{
  "manifest_version": 3,
  "name": "ePhishSense",
  "version": "1.1",
  "description": "Detect phishing emails",
  "permissions": [
    "activeTab",
    "storage"],
  "host_permissions": ["http://localhost:5000/*" ] ,
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.jpeg"
    }
  },
  "icons": {
    "16": "images/icon16.jpeg"
  }
}

