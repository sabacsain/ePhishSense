{
  "manifest_version": 3,
  "name": "ePhishSense",
  "version": "1.0",
  "description": "Detect phishing emails",
  "permissions": [
    "activeTab",
    "storage",
    "http://localhost:5000/*" ],
  "host_permissions": 
    "http://localhost:5000/*",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.jpeg"
    }
  },
  "icons": {
    "16": "images/icon16.jpeg"
  }
}

